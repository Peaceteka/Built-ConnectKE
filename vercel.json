{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static-build"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static-build"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
